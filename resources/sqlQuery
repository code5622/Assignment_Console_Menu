CREATE DATABASE IF NOT EXISTS assignmentdb;

use assignmentdb;
 
DROP TABLE IF EXISTS student;
 
CREATE TABLE student(
	sid int NOT NULL AUTO_INCREMENT,
	sname varchar(20),
	saddr varchar(20),
	sgender varchar(20),
	sdob date,
    PRIMARY KEY(sid)
);

